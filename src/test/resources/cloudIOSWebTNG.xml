<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Web mobile testing Suite">
    <parameter name="appType" value="web"></parameter>
<!--    <parameter name="deviceName" value="LG-H870S"></parameter>-->
<!--    <parameter name="udid" value="LGH870S9c2f6097"></parameter>-->
    <parameter name="platformName" value="iOS"></parameter>
    <parameter name="browserName" value="Safari"></parameter>
    <test name="iOS cloud web tests">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <packages>
            <package name="scenarios"/>
        </packages>
    </test>
</suite>